<% include ../partials/header %>

<div class="post-page-container">
  <div class="post-page-container">
    <div class="post-card">
      <h2 class="city-post-title"><%= post.title %></h2>
      <i class="fas fa-map-marker-alt"></i><a href="/cities/<%= post.city_id %>"><%= post.name %></a>
      <p><%= moment(post.created_at).format("MMM DD YYYY") %></p>
      <p><%= post.body %></p>
    </div>
  </div>
</div>

<% include ../partials/footer %>